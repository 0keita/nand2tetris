/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=w1);
    Nand(a=b, b=b, out=w2);
    Nand(a=w1, b=w2, out=or1);

    Nand(a=a, b=b, out=or2);
    Nand(a=or1, b=or2, out=res);
    Nand(a=res, b=res, out=out);
}
